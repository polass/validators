{
    "name": "polass/validators",
    "description": "Extra validators for Laravel",
    "type": "library",
    "homepage": "https://github.com/polass/validators",
    "keywords": [ "validator", "validation", "laravel" ],
    "authors": [
        {
            "name": "abesuke"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/validation": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
		"illuminate/translation": "~5.1"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Polass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polass\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "license": "MIT"
}
